<%- include('partials/head') %> 
<body>
    <%- include('partials/navbar') %> 
    <h1>Your random num is <%= rand %> </h1>
    <!--  conditions -->
    <% if(rand % 2 === 0){ %>
    <h2>That is an even number</h2>
    <% } else{ %>
    <h2>That is an odd number</h2>
    <% } %> 

    <!-- using ternary operator -->
    <h3>This number is <%= rand % 2 === 0 ? 'Even' : 'Odd' %> </h3>
</body>
</html>